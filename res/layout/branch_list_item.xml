<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_margin="5dip" android:padding="10dip" android:layout_height="140dip" android:layout_width="140dip" android:background="@drawable/branch_logo_bg">
  
		<ImageView   
               android:id="@+id/branch_list_item_logo"   
               android:layout_centerHorizontal="true" android:layout_width="75dip" android:layout_height="75dip">   
         </ImageView>  
         <TextView   
               android:layout_width="wrap_content"   
               android:layout_below="@+id/branch_list_item_logo"   
               android:layout_height="wrap_content"   
               android:text="Army"   
               android:layout_centerHorizontal="true"   
               android:id="@+id/branch_list_item_name" android:textColor="#FFFFFF">  
         </TextView> 
         <TextView   
               android:layout_width="wrap_content"   
               android:layout_below="@+id/branch_list_item_name"   
               android:layout_height="wrap_content"   
               android:text="Army1"   
               android:layout_centerHorizontal="true"   
               android:id="@+id/branch_list_item_locale_name" android:textColor="#FFFFFF" android:textSize="10dip"  android:typeface="sans">  
         </TextView> 
</RelativeLayout>
