<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:orientation="vertical">
  
  	<ImageButton android:id="@+id/btn_back_3" android:layout_gravity="right" android:layout_width="24dip" android:layout_height="24dip" android:layout_marginTop="15dip" android:layout_marginRight="10dip"  android:scaleType="fitCenter" android:background="@drawable/back"></ImageButton>
    
    <ScrollView android:id="@+id/scrollView1" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1">
    	<LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout1" android:layout_height="fill_parent"  android:orientation="vertical" android:padding="5dip">
    
		    <LinearLayout android:layout_width="fill_parent" android:layout_height="100dip" android:id="@+id/rank_deatail_layout1" android:gravity="center_vertical|center_horizontal" android:layout_marginTop="5dip" android:background="@drawable/boder_bg">
		        <ImageView android:layout_width="200dip" android:src="@drawable/icon" android:id="@+id/rank_detail_img" android:layout_height="80dip" android:layout_gravity="center_vertical|center_horizontal"></ImageView>
		    </LinearLayout>
			
		    <TableLayout android:id="@+id/tableLayout1" android:layout_width="fill_parent" android:layout_height="fill_parent">
		        <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="35dip" android:layout_marginTop="5dip" android:background="@drawable/boder_bg">
		        	<TextView android:text="Title" android:layout_gravity="right|center_vertical" android:textColor="#AC8C75" android:textSize="13dip" android:width="100dip" android:gravity="right" android:typeface="sans"></TextView>
		   		
		   			<TextView android:text="TextView" 
		    		  android:id="@+id/rank_detail_title" 
		     		  android:layout_width="fill_parent" android:layout_gravity="center_vertical|center_horizontal" android:gravity="center_vertical|center_horizontal" android:layout_margin="2dip" android:textColor="#FFFFFF" android:textSize="13dip" android:typeface="sans" android:width="150dip" android:paddingLeft="20dip"></TextView>
		        </TableRow>
		        <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="35dip" android:layout_marginTop="5dip" android:background="@drawable/boder_bg">
		        	<TextView android:text="Original Title" android:width="100dip" android:layout_gravity="right|center_vertical" android:textColor="#AC8C75" android:textSize="13dip" android:typeface="sans" android:gravity="right"></TextView>
		   		
		   			<TextView android:text="TextView" 
		    		  android:id="@+id/rank_detail_locale_title" 
		     		  android:layout_width="fill_parent" android:layout_gravity="center_vertical|center_horizontal" android:gravity="center_vertical|center_horizontal" android:layout_margin="2dip" android:textColor="#FFFFFF" android:textSize="13dip" android:typeface="sans" android:width="150dip" android:paddingLeft="20dip"></TextView>
		        </TableRow>
		        <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="35dip" android:layout_marginTop="5dip" android:background="@drawable/boder_bg">
		        	<TextView android:text="Country" android:width="100dip" android:layout_gravity="right|center_vertical" android:textColor="#AC8C75" android:textSize="13dip" android:typeface="sans" android:gravity="right"></TextView>
		   		
		   			<TextView android:text="TextView" 
		    		  android:id="@+id/rank_detail_country" 
		     		  android:layout_width="fill_parent" android:layout_gravity="center_vertical|center_horizontal" android:gravity="center_vertical|center_horizontal" android:layout_margin="2dip" android:textColor="#FFFFFF" android:textSize="13dip" android:typeface="sans" android:width="150dip" android:paddingLeft="20dip"></TextView>
		        </TableRow>
		        <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="35dip" android:layout_marginTop="5dip" android:background="@drawable/boder_bg">
		        	<TextView android:text="Branch" android:width="100dip" android:layout_gravity="right|center_vertical" android:textColor="#AC8C75" android:textSize="13dip" android:typeface="sans" android:gravity="right"></TextView>
		   		
		   			<TextView android:text="TextView" 
		    		  android:id="@+id/rank_detail_branch" 
		     		  android:layout_width="fill_parent" android:layout_gravity="center_vertical|center_horizontal" android:gravity="center_vertical|center_horizontal" android:layout_margin="2dip" android:textColor="#FFFFFF" android:textSize="13dip" android:typeface="sans" android:width="150dip" android:paddingLeft="20dip"></TextView>
		        </TableRow>
		        <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_marginTop="5dip" android:background="@drawable/boder_bg" android:layout_height="wrap_content">
		        	<TextView android:text="Info" android:width="100dip" android:layout_gravity="right|center_vertical" android:textColor="#AC8C75" android:textSize="13dip" android:typeface="sans" android:gravity="right"></TextView>
		   		
		   			<TextView android:text="TextView\n" 
		    		  android:id="@+id/rank_detail_info" 
		     		  android:layout_width="150dip" android:layout_gravity="center_vertical|center_horizontal" android:gravity="center_vertical|center_horizontal" android:layout_margin="2dip" android:textColor="#FFFFFF" android:textSize="13dip" android:typeface="sans" android:paddingLeft="30dip" android:height="60dip"></TextView>
		        </TableRow>
		    </TableLayout>
    	</LinearLayout>
    </ScrollView>
    
    <com.google.ads.AdView android:id="@+id/ad_rank_detail"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:layout_marginBottom="10dip"
                         ads:adUnitId="a14e2ce7576f2a3"
                         ads:adSize="BANNER"/>
</LinearLayout>
